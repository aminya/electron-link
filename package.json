{
  "name": "electron-link",
  "version": "0.4.2",
  "description": "Generates scripts to pass to V8's `mksnapshot` tool.",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha test/**/*.test.js --ui=tdd --compilers js:babel-register",
    "tdd": "electron-mocha test/**/*.test.js --ui=tdd --renderer --interactive --compilers js:babel-register",
    "prepublish": "rimraf lib && babel src -d lib"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-register": "^6.23.0",
    "dedent": "^0.6.0",
    "electron": "5.0.13",
    "electron-mocha": "^3.3.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.7.1",
    "temp": "^0.8.4"
  },
  "dependencies": {
    "acorn": "^7.3.1",
    "ast-util": "^0.6.0",
    "encoding-down": "6.3.0",
    "indent-string": "4.0.0",
    "leveldown": "5.6.0",
    "levelup": "4.4.0",
    "recast": "0.19.1",
    "resolve": "1.17.0",
    "source-map": "0.7.3"
  }
}
